{
  "name": "stampede-cache",
  "version": "0.2.0",
  "description": "Shared cache code between stampede apps",
  "license": "MIT",
  "repository": "davidahouse/stampede-cache",
  "main": "index.js",
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.0.1",
    "eslint-config-google": "^0.11.0",
    "mocha": "^5.2.0",
    "nock": "^10.0.6"
  },
  "scripts": {
    "lint": "eslint $(find . -name \"*.js\" -not -path \"./node_modules/*\" -not -path \"./public/*\")",
    "lint-fix": "eslint --fix $(find . -name \"*.js\" -not -path \"./node_modules/*\" -not -path \"./public/*\")",
    "test": "mocha",
    "test-debug": "mocha --inspect-brk test/lynn-request-tests.js"
  },
  "dependencies": {
    "async-redis": "^1.1.7"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "google"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
        "impliedStrict": true
      },
      "sourceType": "module"
    },
    "rules": {
      "max-len": [
        "error",
        {
          "code": 120
        }
      ],
      "semi": [
        "error",
        "never"
      ]
    }
  }
}
